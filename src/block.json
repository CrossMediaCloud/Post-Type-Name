{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "cross-media-cloud/post-type-name",
	"version": "0.1.0",
	"title": "Post Type Name",
	"category": "widgets",
	"icon": "smiley",
	"description": "Display post type name as a block with styling options.",
	"example": {},
	"supports": {
		"html": false,
		"align": true,
		"alignWide": true,
		"typography": {
			"lineHeight": true,
			"fontSize": true,
			"textAlign": true
		},
		"spacing": {
			"padding": true,
			"margin": true
		},
		"border": {
			"radius": true
		},
		"shadow": true,
		"anchor": true,
		"color": {
			"background": true,
			"text": true,
			"enableContrastChecker": true
		},
		"background": {
			"backgroundImage": true
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": true,
				"style": true,
				"width": true
			}
		}
	},
	"attributes": {
		"prefix": {
			"type": "string",
			"default": ""
		},
		"suffix": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "post-type-name",
	"editorScript": "file:./index.js",
	"render": "file:./render.php"
}
